<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="IAppTheme2" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:windowLightStatusBar">false</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="IWhite" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/status_bar_light</item>
        <item name="colorPrimaryDark">@color/status_bar_light</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="windowActionBar">false</item>
        <item name="android:windowLightStatusBar">true</item>
        <item name="windowNoTitle">true</item>
    </style>

</resources>
